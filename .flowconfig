[ignore]
.*/node_modules/editions/es2015/.*
[options]
esproposal.decorators=ignore
suppress_comment= \\(.\\|\n\\)*\\flow-disable-next-line
; Extensions
module.file_ext=.js
module.file_ext=.jsx
module.name_mapper='^Assets\(.*\)$' -> '<PROJECT_ROOT>/src/client/assets/\1'
module.name_mapper='^Store\(.*\)$' -> '<PROJECT_ROOT>/src/client/store/\1'
module.name_mapper='^Common\(.*\)$' -> '<PROJECT_ROOT>/src/client/common/\1'
module.name_mapper='^Components\(.*\)$' -> '<PROJECT_ROOT>/src/client/components/\1'
module.name_mapper='^~\(.*\)$' -> '<PROJECT_ROOT>/src/client/\1'
